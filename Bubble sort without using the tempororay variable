function bubbleSort(array) {
let temp = array.length - 1;
do {
 for (i = 0; i < array.length; i++) {
		if(array[i]>array[i+1]) {
     array[i] = array[i]+ array[i+1];
     array[i+1] = array[i] - array[i+1];
     array[i] = array[i] - array[i+1];
    }
  }
  temp = temp - 1;
  // console.log(temp)
} while(temp > 0)
  
  console.log(array)
}

bubbleSort([5, 2, 1, 3, 4])
